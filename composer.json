{
  "name": "evelution87/meta",
  "description": "Adds morphable meta capabilities to Laravel models",
  "keywords": [
    "evelution",
    "meta"
  ],
  "homepage": "https://github.com/evelution/meta",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Craig Eve",
      "email": "craig@evelution.net",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Evelution\\Meta\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Evelution\\Meta\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../*"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Evelution\\Meta\\MetaServiceProvider"
      ],
      "aliases": {
        "Meta": "Evelution\\Meta\\MetaFacade"
      }
    }
  }
}
